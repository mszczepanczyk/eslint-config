{
  "name": "@mariusz.sh/eslint-config",
  "version": "3.0.0",
  "main": "index.js",
  "homepage": "https://github.com/mszczepanczyk/eslint-config",
  "repository": "https://github.com/mszczepanczyk/eslint-config.git",
  "author": "Mariusz Szczepa≈Ñczyk <m@mariusz.sh>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "lint": "npx eslint . --max-warnings 0",
    "format": "npx eslint . --fix",
    "publish": "npm publish",
    "test": "npx tap --reporter=terse",
    "test:snapshot": "TAP_SNAPSHOT=1 npm run test",
    "test:watch": "npx chokidar *.js *.json fixtures --initial -c 'npm run test'"
  },
  "dependencies": {
    "@stylistic/eslint-plugin-js": "2.12.1",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react": "7.37.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "typescript-eslint": "8.19.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "chokidar-cli": "3.0.0",
    "eslint": "9.17.0",
    "prettier": "3.4.2",
    "tap": "21.0.1",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  }
}
